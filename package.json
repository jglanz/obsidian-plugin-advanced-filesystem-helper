{
  "name": "obsidian-advanced-filesystem-helper",
  "version": "1.5.1",
  "description": "Create notes in chosen folder for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "lint": "eslint . --ext .ts && tsc --noEmit && prettier --check src/",
    "dev": "concurrently 'tsc -noEmit -skipLibCheck -w' 'node esbuild.config.mjs'",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "builtin-modules": "^3.2.0",
    "concurrently": "8.2.2",
    "esbuild": "^0.13.12",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "obsidian": "^0.14.6",
    "obsidian-plugin-cli": "^0.8.1",
    "prettier": "^2.6.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}